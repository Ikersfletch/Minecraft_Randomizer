{
  "format_version": "1.16.0",
  "minecraft:entity": {
    "components": {
      "minecraft:jump.static": {},
      "minecraft:on_friendly_anger": {
        "event": "minecraft:defend_wandering_trader",
        "target": "self"
      },
      "minecraft:physics": {},
      "minecraft:nameable": {},
      "minecraft:can_climb": {},
      "minecraft:breathable": {
        "total_supply": 15,
        "suffocate_time": 0
      },
      "minecraft:movement.basic": {},
      "minecraft:type_family": {"family": [
        "enderman",
        "monster",
        "mob"
      ]},
      "minecraft:behavior.look_at_player": {
        "probability": 8,
        "look_distance": 8,
        "priority": 8
      },
      "minecraft:behavior.nearest_attackable_target": {
        "attack_interval": 10,
        "entity_types": [{
          "max_dist": 64,
          "filters": {
            "test": "is_family",
            "subject": "other",
            "value": "endermite"
          }
        }],
        "must_see": true,
        "priority": 5
      },
      "minecraft:teleport": {
        "random_teleports": true,
        "max_random_teleport_time": 30,
        "target_distance": 16,
        "light_teleport_chance": 0.05,
        "target_teleport_chance": 0.05,
        "random_teleport_cube": [
          32,
          32,
          32
        ]
      },
      "minecraft:navigation.walk": {
        "avoid_water": true,
        "can_path_over_water": false
      },
      "minecraft:behavior.enderman_take_block": {"priority": 11},
      "minecraft:loot": {"table": "loot_tables/entities/enderman.json"},
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      },
      "minecraft:is_hidden_when_invisible": {},
      "minecraft:behavior.random_stroll": {
        "speed_multiplier": 1,
        "priority": 7
      },
      "minecraft:is_shaking": {},
      "minecraft:behavior.random_look_around": {"priority": 8},
      "minecraft:behavior.take_flower": {"priority": 7},
      "minecraft:health": {
        "max": 40,
        "value": 40
      },
      "minecraft:behavior.hurt_by_target": {"priority": 3},
      "minecraft:collision_box": {
        "width": 0.6,
        "height": 2.9
      },
      "minecraft:behavior.enderman_leave_block": {"priority": 10},
      "minecraft:burns_in_daylight": {},
      "minecraft:despawn": {"despawn_from_distance": {}},
      "minecraft:behavior.float": {"priority": 0},
      "minecraft:experience_reward": {"on_death": "query.last_hit_by_player ? 5 : 0"},
      "minecraft:hurt_on_condition": {"damage_conditions": [
        {
          "damage_per_tick": 4,
          "cause": "lava",
          "filters": {
            "test": "in_lava",
            "subject": "self",
            "value": true,
            "operator": "=="
          }
        },
        {
          "damage_per_tick": 1,
          "cause": "drowning",
          "filters": {
            "test": "in_water_or_rain",
            "value": true,
            "operator": "=="
          }
        }
      ]},
      "minecraft:lookat": {
        "search_radius": 64,
        "set_target": true,
        "look_cooldown": 5,
        "filters": {"all_of": [
          {
            "test": "is_family",
            "subject": "other",
            "value": "player"
          },
          {
            "test": "has_equipment",
            "subject": "other",
            "domain": "head",
            "value": "carved_pumpkin",
            "operator": "not"
          }
        ]}
      },
      "minecraft:attack": {"damage": 7},
      "minecraft:follow_range": {
        "max": 32,
        "value": 32
      }
    },
    "component_groups": {
      "minecraft:enderman_angry": {
        "minecraft:movement": {"value": 0.45},
        "minecraft:behavior.follow_parent": {
          "speed_multiplier": 1.1,
          "priority": 11
        },
        "minecraft:on_friendly_anger": {
          "event": "minecraft:defend_wandering_trader",
          "target": "self"
        },
        "minecraft:attack_cooldown": {
          "attack_cooldown_time": [
            30,
            120
          ],
          "attack_cooldown_complete_event": {
            "event": "attack_cooldown_complete_event",
            "target": "self"
          }
        },
        "minecraft:behavior.melee_attack": {"priority": 2},
        "minecraft:angry": {
          "duration": 25,
          "calm_event": {
            "event": "minecraft:on_calm",
            "target": "self"
          }
        }
      },
      "minecraft:enderman_calm": {
        "minecraft:movement": {"value": 0.3},
        "minecraft:physics": {"has_gravity": false},
        "minecraft:on_target_acquired": {
          "event": "minecraft:become_angry",
          "target": "self"
        },
        "minecraft:is_tamed": {},
        "minecraft:behavior.slime_float": {
          "speed_multiplier": 1.2,
          "jump_chance_percentage": 0.8,
          "priority": 1
        },
        "minecraft:health": {
          "max": 1,
          "value": 1
        },
        "minecraft:behavior.send_event": {
          "event_choices": [{
            "cooldown_time": 5,
            "sequence": [{
              "base_delay": 2,
              "sound_event": "prepare.wololo",
              "event": "wololo"
            }],
            "particle_color": "#FFB38033",
            "min_activation_range": 0,
            "weight": 3,
            "start_sound_event": "cast.spell",
            "filters": {"all_of": [
              {
                "test": "is_family",
                "subject": "other",
                "value": "sheep"
              },
              {
                "test": "is_color",
                "subject": "other",
                "value": "blue"
              }
            ]},
            "cast_duration": 3,
            "max_activation_range": 16
          }],
          "priority": 3
        },
        "minecraft:behavior.flee_sun": {
          "speed_multiplier": 1,
          "priority": 2
        }
      }
    },
    "description": {
      "identifier": "minecraft:enderman",
      "is_spawnable": true,
      "is_summonable": true,
      "is_experimental": false
    },
    "events": {
      "minecraft:entity_spawned": {
        "add": {"component_groups": ["minecraft:enderman_calm"]},
        "remove": {}
      },
      "minecraft:on_calm": {
        "add": {"component_groups": ["minecraft:enderman_calm"]},
        "remove": {"component_groups": ["minecraft:enderman_angry"]}
      },
      "minecraft:become_angry": {
        "add": {"component_groups": ["minecraft:enderman_angry"]},
        "remove": {"component_groups": ["minecraft:enderman_calm"]}
      }
    }
  }
}