{
  "format_version": "1.12.0",
  "minecraft:entity": {
    "components": {
      "minecraft:equippable": {"slots": [{
        "item": "carpet",
        "slot": 1,
        "accepted_items": ["carpet"]
      }]},
      "minecraft:physics": {},
      "minecraft:can_fly": {},
      "minecraft:is_stackable": {"value": true},
      "minecraft:type_family": {"family": [
        "villager",
        "artisan",
        "leatherworker",
        "mob"
      ]},
      "minecraft:collision_box": {
        "width": 0.98,
        "height": 0.98
      },
      "minecraft:explode": {
        "fuse_lit": true,
        "fuse_length": 4,
        "power": 4,
        "causes_fire": false
      },
      "minecraft:loot": {"table": "loot_tables/entities/sea_turtle.json"},
      "minecraft:behavior.squid_idle": {"priority": 2},
      "minecraft:navigation.generic": {
        "can_walk": true,
        "can_swim": true,
        "avoid_damage_blocks": true,
        "can_sink": false,
        "can_path_over_water": false,
        "is_amphibious": true
      },
      "minecraft:pushable": {
        "is_pushable": false,
        "is_pushable_by_piston": true
      },
      "minecraft:behavior.float_wander": {
        "float_duration": [
          0.1,
          0.35
        ],
        "random_reselect": true,
        "xz_dist": 10,
        "y_dist": 7,
        "y_offset": -2
      },
      "minecraft:on_hurt": {
        "event": "minecraft:ranged_mode",
        "target": "self"
      },
      "minecraft:interact": {"interactions": {
        "use_item": true,
        "interact_text": "action.interact.cure",
        "on_interact": {
          "filters": {"all_of": [
            {
              "test": "has_equipment",
              "subject": "other",
              "domain": "hand",
              "value": "golden_apple"
            },
            {
              "test": "has_component",
              "subject": "self",
              "value": "minecraft:effect.weakness"
            }
          ]},
          "event": "villager_converted",
          "target": "self"
        }
      }}
    },
    "component_groups": {"from_explosion": {
      "minecraft:behavior.move_to_random_block": {
        "speed_multiplier": 0.55,
        "block_distance": 512,
        "within_radius": 8,
        "priority": 5
      },
      "minecraft:behavior.move_towards_target": {
        "speed_multiplier": 0.9,
        "within_radius": 32,
        "priority": 2
      },
      "minecraft:behavior.celebrate": {
        "duration": 30,
        "jump_interval": {
          "range_min": 1,
          "range_max": 3.5
        },
        "celebration_sound": "celebrate",
        "sound_interval": {
          "range_min": 2,
          "range_max": 7
        },
        "on_celebration_end_event": {
          "event": "minecraft:stop_celebrating",
          "target": "self"
        },
        "priority": 5
      },
      "minecraft:explode": {
        "fuse_lit": true,
        "fuse_length": {
          "range_min": 0.5,
          "range_max": 2
        },
        "power": 4,
        "causes_fire": false
      },
      "minecraft:behavior.look_at_entity": {
        "angle_of_view_horizontal": 45,
        "look_distance": 8,
        "filters": {
          "test": "is_family",
          "subject": "other",
          "value": "mob"
        },
        "priority": 10
      }
    }},
    "description": {
      "identifier": "minecraft:tnt",
      "is_spawnable": false,
      "is_summonable": true,
      "is_experimental": false
    },
    "events": {"from_explosion": {"add": {"component_groups": ["from_explosion"]}}}
  }
}