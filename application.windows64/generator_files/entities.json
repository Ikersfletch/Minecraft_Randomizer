        "priority": 3,
        "within_radius": 16.0,
        "entity_types": [
          {
            "filters": {
              "all_of": [
                {
                  "test": "is_family",
                  "subject": "other",
                  "operator": "!=",
                  "value": "zoglin"
                },
                {
                  "test": "is_family",
                  "subject": "other",
                  "operator": "!=",
